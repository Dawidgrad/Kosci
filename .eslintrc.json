{
	"env": {
		"browser": true,
		"es2021": true,
		"jquery": true,
		"cypress/globals": true
	},
	"extends": ["airbnb-base", "plugin:cypress/recommended"],
	"parserOptions": {
		"ecmaVersion": 12
	},
	"rules": {
		"linebreak-style": ["error", "windows"],
		"indent": ["error", "tab"],
		"no-tabs": 0,
		"operator-linebreak": 0,
		"object-curly-newline": 0,
		"comma-dangle": 0,
		"quote-props": 0
	}
}
